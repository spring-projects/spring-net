<?xml version="1.0" encoding="utf-8" ?>
<xs:schema targetNamespace="http://schemas.springframework.net/testobject" elementFormDefault="qualified"
           xmlns="http://schemas.springframework.net/testobject"
           xmlns:mstns="http://schemas.springframework.net/testobject"
           xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="testobject">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="age" type="xs:positiveInteger" />
                <xs:element name="name" type="xs:string" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>