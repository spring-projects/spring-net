<?xml version="1.0" encoding="UTF-8"?>

<objects xmlns="https://www.springframework.net" 
	xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="https://www.springframework.net https://www.springframework.net/xsd/spring-objects.xsd">

	<!-- Simple target -->
	<object id="test" type="Spring.Objects.TestObject, Spring.Core.Tests">
		<property name="name">
			<value>custom</value>
		</property>
		<property name="age">
			<value>666</value>
		</property>
	</object>
	<!--
		Check that invoker is automatically added to wrap target.
		Non pointcut object name should be wrapped in invoker.
	-->
	<object id="numberTestObject" type="Spring.Objects.NumberTestObject, Spring.Core.Tests" />
</objects>
