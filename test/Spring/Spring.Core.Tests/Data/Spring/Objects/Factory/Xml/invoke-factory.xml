<?xml version="1.0" encoding="utf-8" ?>

<objects xmlns="http://www.springframework.net" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.springframework.net https://www.springframework.net/xsd/spring-objects.xsd">
    <object id="culture" type="Spring.Objects.Factory.Config.PropertyRetrievingFactoryObject, Spring.Core">
        <property name="TargetType">
            <value>System.Globalization.CultureInfo, Mscorlib</value>
        </property>
        <property name="TargetProperty">
            <value>InstalledUICulture</value>
        </property>
    </object>
    <object id="culturalObject" type="Spring.Objects.TestObject, Spring.Core.Tests">
        <property name="MyCulture">
            <ref object="culture" />
        </property>
    </object>
</objects>
