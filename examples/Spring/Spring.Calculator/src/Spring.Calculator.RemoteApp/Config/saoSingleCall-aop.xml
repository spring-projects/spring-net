<?xml version="1.0" encoding="utf-8" ?>
<objects	
	xmlns="http://www.springframework.net" 
	xmlns:r="http://www.springframework.net/remoting">
	
	<description>Registers the calculator service proxied as a SAO in 'SingleCall' mode.</description>
	
	<r:saoExporter 
		targetName="prototypeCalculatorWeaved" 
		serviceName="RemotedSaoSingleCallCalculatorWeaved" />
		
	<!--
		Old fashion way without remoting config parser...
	<object name="saoSingleCallCalculatorWeaved" type="Spring.Remoting.SaoExporter, Spring.Services">
		<property name="TargetName" value="prototypeCalculatorWeaved" />
		<property name="ServiceName" value="RemotedSaoSingleCallCalculatorWeaved" />
	</object>
	-->
	
</objects>